  "private": true,
  "homepage": "https://Gkving.github.io/rololex",
  "dependencies": {
    "gh-pages": "^6.3.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-scripts": "4.0.0"
  },
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
